						argument = New Microsoft.CodeAnalysis.VisualBasic.BoundDirectCast(node, argument, Microsoft.CodeAnalysis.VisualBasic.ConversionKind.Identity, targetType, False)
					End If
					boundDirectCast = argument
				End If
			End If
