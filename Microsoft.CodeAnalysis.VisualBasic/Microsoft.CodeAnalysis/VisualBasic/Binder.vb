					instance.Add(ErrorFactory.ErrorInfo(ERRID.ERR_OverloadCandidate2, New [Object]() { CustomSymbolDisplayFormatter.WithContainingType(item), diagnosticInfo }))
				End If
				num1 = num1 + 1
