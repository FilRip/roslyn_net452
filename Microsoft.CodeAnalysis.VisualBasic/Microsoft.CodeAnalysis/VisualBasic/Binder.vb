								End If
								Dim binder As Microsoft.CodeAnalysis.VisualBasic.Binder = BinderBuilder.CreateBinderForInitializer(topLevelCodeBinder, symbol1, empty)
								If (visualBasicSyntaxNode.Kind() = SyntaxKind.ModifiedIdentifier) Then
