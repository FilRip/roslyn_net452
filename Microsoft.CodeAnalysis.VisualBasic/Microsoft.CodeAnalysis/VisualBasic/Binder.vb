					If (current.Kind <> SymbolKind.Field OrElse current.IsShared OrElse DirectCast(current, FieldSymbol).IsReadOnly) Then
						Continue While
					End If
					flag = True
					Return flag
				End While
			End If
			flag = False
			Return flag
		End Function

